	   if (arrow26S1.getY()<100 && arrow26S1.getY()>-80){points+=1;println(points);remove(arrow26);remove(arrow26S1);remove(arrow26S2);remove(arrow26L);remove(arrow26C);addsArrowTicker26 = 0;}
	   if (arrow27S1.getY()<100 && arrow27S1.getY()>-80){points+=1;println(points);remove(arrow27);remove(arrow27S1);remove(arrow27S2);remove(arrow27L);remove(arrow27C);addsArrowTicker27 = 0;}
	   if (arrow28S1.getY()<100 && arrow28S1.getY()>-80){points+=1;println(points);remove(arrow28);remove(arrow28S1);remove(arrow28S2);remove(arrow28L);remove(arrow28C);addsArrowTicker28 = 0;}     
	   if (arrow29S1.getY()<100 && arrow29S1.getY()>-80){points+=1;println(points);remove(arrow29);remove(arrow29S1);remove(arrow29S2);remove(arrow29L);remove(arrow29C);addsArrowTicker29 = 0;}     
	   if (arrow30S1.getY()<100 && arrow30S1.getY()>-80){points+=1;println(points);remove(arrow30);remove(arrow30S1);remove(arrow30S2);remove(arrow30L);remove(arrow30C);addsArrowTicker30 = 0;}
        if (time>Amount){println("if (time == " + time + "){arrowY22 = 400;}// Up");}	  }
        
    if (e.keyCode == Keyboard.RIGHT || e.keyCode == Keyboard.letter('d')) {
	   if (arrow31S1.getY()<100 && arrow31S1.getY()>-80){points+=1;println(points);remove(arrow31);remove(arrow31S1);remove(arrow31S2);remove(arrow31L);remove(arrow31C);addsArrowTicker31 = 0;}
