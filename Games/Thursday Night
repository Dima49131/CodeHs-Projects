    if (arrowY40 <= -50){X = 360;Y = 510;arrow40.setPosition(X, Y);arrow40S1.setPosition(X, Y+25);arrow40S2.setPosition(X-20, Y+13);arrow40L.setPosition(X-30, Y+12);arrow40C.setPosition(X-10, Y+10);if (addsArrowTicker40 == 0){addsArrowTicker40 = 1;}}
}




function arrows(X1,Y1, Rota1, fill){ // left 1
    if (Rota1 == 0){
        arrow = new Oval(50, 15);arrow.setPosition(X1, Y1);arrow.setColor(fill);arrow.rotate(-35);
        add(arrow);
        
        arrowS1 = new Oval(50, 15);arrowS1.setPosition(X1, Y1+25);arrowS1.setColor(fill);arrowS1.rotate(35);
        add(arrowS1);
        
        arrowS2 = new Oval(55, 15);arrowS2.setPosition(X1+20, Y1+13);arrowS2.setColor(fill);
        add(arrowS2);
        
        arrowL = new Oval(75, 20);arrowL.setPosition(X1+30, Y1+12);arrowL.setColor(fill);
        add(arrowL);
        
        arrowC = new Oval(20, 40);arrowC.setPosition(X1+10, Y1+10);arrowC.setColor(fill);
        add(arrowC);
        
        arrowS2.rotate(90);arrowS1.rotate(180);arrow.rotate(180);}
    if (Rota1 == 90){
        arrow = new Oval(50, 15);arrow.setPosition(X1+14, Y1-15);arrow.setColor(fill);arrow.rotate(-35);
        add(arrow);
        
        arrowS1 = new Oval(50, 15);arrowS1.setPosition(X1-14, Y1-15);arrowS1.setColor(fill);arrowS1.rotate(35);
        add(arrowS1);
        
        arrowS2 = new Oval(55, 15);arrowS2.setPosition(X1, Y1+4);arrowS2.setColor(fill);
        add(arrowS2);
        
        arrowL = new Oval(75, 20);arrowL.setPosition(X1, Y1+10);arrowL.setColor(fill);
        add(arrowL);
        
        arrowC = new Oval(20, 40);arrowC.setPosition(X1, Y1);arrowC.setColor(fill);
        add(arrowC);
        
        arrowS2.rotate(180);arrowS1.rotate(270);arrow.rotate(270);arrowL.rotate(90);arrowC.rotate(90);}
    if (Rota1 == 180){
        arrow = new Oval(50, 15);arrow.setPosition(X1, Y1);arrow.setColor(fill);arrow.rotate(-35);
        add(arrow);
        
        arrowS1 = new Oval(50, 15);arrowS1.setPosition(X1, Y1+25);arrowS1.setColor(fill);arrowS1.rotate(35);
        add(arrowS1);
        
        arrowS2 = new Oval(55, 15);arrowS2.setPosition(X1-20, Y1+13);arrowS2.setColor(fill);
        add(arrowS2);
        
        arrowL = new Oval(75, 20);arrowL.setPosition(X1-30, Y1+12);arrowL.setColor(fill);
        add(arrowL);
        
        arrowC = new Oval(20, 40);arrowC.setPosition(X1-10, Y1+10);arrowC.setColor(fill);
        add(arrowC);

        arrowS2.rotate(90);arrowS1.rotate(110);arrow.rotate(250);}
    if (Rota1 == 270){
        arrow = new Oval(50, 15);arrow.setPosition(X1+14, Y1-15);arrow.setColor(fill);arrow.rotate(-35);
        add(arrow);
        
        arrowS1 = new Oval(50, 15);arrowS1.setPosition(X1-14, Y1-15);arrowS1.setColor(fill);arrowS1.rotate(35);
        add(arrowS1);
        
        arrowS2 = new Oval(58, 15);arrowS2.setPosition(X1, Y1-34);arrowS2.setColor(fill);
        add(arrowS2);
        
        arrowL = new Oval(75, 20);arrowL.setPosition(X1, Y1-40);arrowL.setColor(fill);
        add(arrowL);
        
        arrowC = new Oval(20, 40);arrowC.setPosition(X1, Y1-20);arrowC.setColor(fill);
        add(arrowC);
        
        arrowS2.rotate(0);arrowS1.rotate(200);arrow.rotate(160);arrowL.rotate(90);arrowC.rotate(90);
    }
}

