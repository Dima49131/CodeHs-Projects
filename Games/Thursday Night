	   if (arrow33S1.getY()<100 && arrow33S1.getY()>-80){points+=1;println(points);remove(arrow33);remove(arrow33S1);remove(arrow33S2);remove(arrow33L);remove(arrow33C);addsArrowTicker33 = 0;}
	   if (arrow34S1.getY()<100 && arrow34S1.getY()>-80){points+=1;println(points);remove(arrow34);remove(arrow34S1);remove(arrow34S2);remove(arrow34L);remove(arrow34C);addsArrowTicker34 = 0;}
