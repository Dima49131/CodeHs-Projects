	   if (arrow14S1.getY()<140 && arrow14S1.getY()>-80){points+=1;println(points);remove(arrow14);remove(arrow14S1);remove(arrow14S2);remove(arrow14L);remove(arrow14C);addsArrowTicker14 = 0;}
