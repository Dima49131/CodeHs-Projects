var startTimer = 0;


function start(){

    keyDownMethod(keyDown);
    mouseClickMethod(startGame);

}


function startGame(){// home computer speed : 
    speedOfGame = -7; // for chromebook put at -16 -- seems to be best for now -- points 111
    
    
    if (startTimer == 0){setTimer(onStep,150);startTimer+=1;}
    
    
}


