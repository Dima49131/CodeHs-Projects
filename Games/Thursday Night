	   if (arrow36S1.getY()<100 && arrow36S1.getY()>-80){points+=1;println(points);remove(arrow36);remove(arrow36S1);remove(arrow36S2);remove(arrow36L);remove(arrow36C);addsArrowTicker36 = 0;}
	   if (arrow37S1.getY()<100 && arrow37S1.getY()>-80){points+=1;println(points);remove(arrow37);remove(arrow37S1);remove(arrow37S2);remove(arrow37L);remove(arrow37C);addsArrowTicker37 = 0;}
	   if (arrow38S1.getY()<100 && arrow38S1.getY()>-80){points+=1;println(points);remove(arrow38);remove(arrow38S1);remove(arrow38S2);remove(arrow38L);remove(arrow38C);addsArrowTicker38 = 0;}     
	   if (arrow39S1.getY()<100 && arrow39S1.getY()>-80){points+=1;println(points);remove(arrow39);remove(arrow39S1);remove(arrow39S2);remove(arrow39L);remove(arrow39C);addsArrowTicker39 = 0;}     
	   if (arrow40S1.getY()<100 && arrow40S1.getY()>-80){points+=1;println(points);remove(arrow40);remove(arrow40S1);remove(arrow40S2);remove(arrow40L);remove(arrow40C);addsArrowTicker40 = 0;}
        if (time>Amount){println("if (time == " + time + "){arrowY33 = 400;}// Right");}    }
        
