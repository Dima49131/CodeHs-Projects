        if (time>Amount){println("if (time == " + time + "){arrowY12 = 400;}// Down");}	  }
        
	if (e.keyCode == Keyboard.UP || e.keyCode == Keyboard.letter('w')) {
	   if (arrow21S1.getY()<100 && arrow21S1.getY()>-80){points+=1;println(points);remove(arrow21);remove(arrow21S1);remove(arrow21S2);remove(arrow21L);remove(arrow21C);addsArrowTicker21 = 0;}
	   if (arrow22S1.getY()<100 && arrow22S1.getY()>-80){points+=1;println(points);remove(arrow22);remove(arrow22S1);remove(arrow22S2);remove(arrow22L);remove(arrow22C);addsArrowTicker22 = 0;}
