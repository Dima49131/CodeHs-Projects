function onStep(){
    if (addsArrowTicker == 1){add(arrow);add(arrowS1);add(arrowS2);add(arrowL);add(arrowC);remove(rect);add(rect);addsArrowTicker = 2;} // left 1
    if (addsArrowTicker2 == 1){add(arrow2);add(arrow2S1);add(arrow2S2);add(arrow2L);add(arrow2C);remove(rect);add(rect);addsArrowTicker2 = 2;}// left 2
    if (addsArrowTicker3 == 1){add(arrow3);add(arrow3S1);add(arrow3S2);add(arrow3L);add(arrow3C);remove(rect);add(rect);addsArrowTicker3 = 2;}// left 3
    if (addsArrowTicker4 == 1){add(arrow4);add(arrow4S1);add(arrow4S2);add(arrow4L);add(arrow4C);remove(rect);add(rect);addsArrowTicker4 = 2;}// left 4
    if (addsArrowTicker5 == 1){add(arrow5);add(arrow5S1);add(arrow5S2);add(arrow5L);add(arrow5C);remove(rect);add(rect);addsArrowTicker5 = 2;}// left 5
    if (addsArrowTicker6 == 1){add(arrow6);add(arrow6S1);add(arrow6S2);add(arrow6L);add(arrow6C);remove(rect);add(rect);addsArrowTicker6 = 2;}// left 6
    if (addsArrowTicker7 == 1){add(arrow7);add(arrow7S1);add(arrow7S2);add(arrow7L);add(arrow7C);remove(rect);add(rect);addsArrowTicker7 = 2;}// left 7 
    if (addsArrowTicker8 == 1){add(arrow8);add(arrow8S1);add(arrow8S2);add(arrow8L);add(arrow8C);remove(rect);add(rect);addsArrowTicker8 = 2;}// left 8 
    if (addsArrowTicker9 == 1){add(arrow9);add(arrow9S1);add(arrow9S2);add(arrow9L);add(arrow9C);remove(rect);add(rect);addsArrowTicker9 = 2;}// left 9 
    if (addsArrowTicker10 == 1){add(arrow10);add(arrow10S1);add(arrow10S2);add(arrow10L);add(arrow10C);remove(rect);add(rect);addsArrowTicker10 = 2;} // left 10
    if (addsArrowTicker11 == 1){add(arrow11);add(arrow11S1);add(arrow11S2);add(arrow11L);add(arrow11C);remove(rect);add(rect);addsArrowTicker11 = 2;}// down 1
    if (addsArrowTicker12 == 1){add(arrow12);add(arrow12S1);add(arrow12S2);add(arrow12L);add(arrow12C);remove(rect);add(rect);addsArrowTicker12 = 2;}// down 2
    if (addsArrowTicker13 == 1){add(arrow13);add(arrow13S1);add(arrow13S2);add(arrow13L);add(arrow13C);remove(rect);add(rect);addsArrowTicker13 = 2;}// down 3
    if (addsArrowTicker14 == 1){add(arrow14);add(arrow14S1);add(arrow14S2);add(arrow14L);add(arrow14C);remove(rect);add(rect);addsArrowTicker14 = 2;}// down 4
    if (addsArrowTicker15 == 1){add(arrow15);add(arrow15S1);add(arrow15S2);add(arrow15L);add(arrow15C);remove(rect);add(rect);addsArrowTicker15 = 2;}// down 5
