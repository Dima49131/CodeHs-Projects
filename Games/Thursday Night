function keyDown(e) {
	if (e.keyCode == Keyboard.LEFT || e.keyCode == Keyboard.letter('a')){
	    if (arrowS1.getY()<110 && arrowS1.getY()>-20){points+=1;println(points);remove(arrow);remove(arrowS1);remove(arrowS2);remove(arrowL);remove(arrowC);addsArrowTicker = 0;}
        if (arrow2S1.getY()<110 && arrow2S1.getY()>-20){points+=1;println(points);remove(arrow2);remove(arrow2S1);remove(arrow2S2);remove(arrow2L);remove(arrow2C);addsArrowTicker2 = 0;}
        if (arrow3S1.getY()<110 && arrow3S1.getY()>-20){points+=1;println(points);remove(arrow3);remove(arrow3S1);remove(arrow3S2);remove(arrow3L);remove(arrow3C);addsArrowTicker3 = 0;}
        if (arrow4S1.getY()<110 && arrow4S1.getY()>-20){points+=1;println(points);remove(arrow4);remove(arrow4S1);remove(arrow4S2);remove(arrow4L);remove(arrow4C);addsArrowTicker4 = 0;}
        if (arrow5S1.getY()<110 && arrow5S1.getY()>-20){points+=1;println(points);remove(arrow5);remove(arrow5S1);remove(arrow5S2);remove(arrow5L);remove(arrow5C);addsArrowTicker5 = 0;}
        if (arrow6S1.getY()<110 && arrow6S1.getY()>-20){points+=1;println(points);remove(arrow6);remove(arrow6S1);remove(arrow6S2);remove(arrow6L);remove(arrow6C);addsArrowTicker6 = 0;}
        if (arrow7S1.getY()<110 && arrow7S1.getY()>-20){points+=1;println(points);remove(arrow7);remove(arrow7S1);remove(arrow7S2);remove(arrow7L);remove(arrow7C);addsArrowTicker7 = 0;}
        if (arrow8S1.getY()<110 && arrow8S1.getY()>-20){points+=1;println(points);remove(arrow8);remove(arrow8S1);remove(arrow8S2);remove(arrow8L);remove(arrow8C);addsArrowTicker8 = 0;}
        if (arrow9S1.getY()<110 && arrow9S1.getY()>-20){points+=1;println(points);remove(arrow9);remove(arrow9S1);remove(arrow9S2);remove(arrow9L);remove(arrow9C);addsArrowTicker9 = 0;}
        if (arrow10S1.getY()<110 && arrow10S1.getY()>-20){points+=1;println(points);remove(arrow10);remove(arrow10S1);remove(arrow10S2);remove(arrow10L);remove(arrow10C);addsArrowTicker10 = 0;}
	    if (time>Amount){println("if (time == " + time + "){arrowY2 = 400;}// Left");}  }
	    
	if (e.keyCode == Keyboard.DOWN || e.keyCode == Keyboard.letter('s')) {
	   if (arrow11S1.getY()<140 && arrow11S1.getY()>-80){points+=1;println(points);remove(arrow11);remove(arrow11S1);remove(arrow11S2);remove(arrow11L);remove(arrow11C);addsArrowTicker11 = 0;}
