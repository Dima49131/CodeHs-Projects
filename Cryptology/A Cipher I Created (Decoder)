var input = readLine("");
var x = 0;

var num = '';
var sqiggleCount = 0;


for (var i = 0; i < input.length; i++){

    if (input[i] == 1 ||input[i] == 2 ||input[i] == 3 ||input[i] == 4 ||input[i] == 5 ||
        input[i] == 6 ||input[i] == 7 ||input[i] == 8 ||input[i] == 9 ||input[i] == '0'){
            if (sqiggleCount == x){num+=input[i];}
        }
    if (input[i] == '~'){
        sqiggleCount++;x++;
        printC();

        num = '';
    }
    if (input[i+1] == '_'){print("_");}
    if (input[i] == '~' && input[i+2] == '~'){print('~');}
    if (input[i] == '-'){print(' - ');}
    if (input[i] == '|'){print('|');}

}


println();


function printC(){
    var factor = 1;
    while(num > 26){
        if (num > 26 * factor){num-= (26 * factor);}
        factor*=10;
        if (num < factor && num > 26){factor = 1;}
}
    print(num);
}


