setTimer(animateBackground,24);
var anoTicker = 0;
function animateBackground(){
    anoTicker++;
    if (anoTicker == 0){add(bdAno2);remove(bdAno1);}
    if (anoTicker == 1){add(bdAno3);remove(bdAno2);}
    if (anoTicker == 2){add(bdAno4);remove(bdAno3);}
    if (anoTicker == 3){add(bdAno5);remove(bdAno4);}
