    if (anoTicker == 32){add(bdAno34);remove(bdAno33);}
    if (anoTicker == 33){add(bdAno35);remove(bdAno34);}
    if (anoTicker == 34){add(bdAno36);remove(bdAno35);}
    if (anoTicker == 35){add(bdAno37);remove(bdAno36);}
    if (anoTicker == 36){add(bdAno38);remove(bdAno37);}
    if (anoTicker == 37){add(bdAno39);remove(bdAno38);}
    if (anoTicker == 38){add(bdAno40);remove(bdAno39);}
    if (anoTicker == 39){add(bdAno41);remove(bdAno40);}
