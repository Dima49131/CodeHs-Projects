    if (anoTicker == 9){add(bdAno11);remove(bdAno10);}
    if (anoTicker == 10){add(bdAno12);remove(bdAno11);}
    if (anoTicker == 11){add(bdAno13);remove(bdAno12);}
    if (anoTicker == 12){add(bdAno14);remove(bdAno13);}
    if (anoTicker == 13){add(bdAno15);remove(bdAno14);}
    if (anoTicker == 14){add(bdAno16);remove(bdAno15);}
    if (anoTicker == 15){add(bdAno17);remove(bdAno16);}
    if (anoTicker == 16){add(bdAno18);remove(bdAno17);}
    if (anoTicker == 17){add(bdAno19);remove(bdAno18);}
    if (anoTicker == 18){add(bdAno20);remove(bdAno19);}
    if (anoTicker == 19){add(bdAno21);remove(bdAno20);}
    if (anoTicker == 20){add(bdAno22);remove(bdAno21);}
    if (anoTicker == 21){add(bdAno23);remove(bdAno22);}
    if (anoTicker == 22){add(bdAno24);remove(bdAno23);}
    if (anoTicker == 23){add(bdAno25);remove(bdAno24);}
    if (anoTicker == 24){add(bdAno26);remove(bdAno25);}
    if (anoTicker == 25){add(bdAno27);remove(bdAno26);}
    if (anoTicker == 26){add(bdAno28);remove(bdAno27);}
    if (anoTicker == 27){add(bdAno29);remove(bdAno28);}
    if (anoTicker == 28){add(bdAno30);remove(bdAno29);}
    if (anoTicker == 29){add(bdAno31);remove(bdAno30);}
    if (anoTicker == 30){add(bdAno32);remove(bdAno31);}
