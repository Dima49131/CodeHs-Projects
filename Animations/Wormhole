    if (anoTicker == 96){add(bdefAno98);remove(bdefAno97);add(circle);remove(circle2);}
    if (anoTicker == 97){add(bdefAno99);remove(bdefAno98);add(circle2);remove(circle);}
    if (anoTicker >= 98){anoTicker = -2;}
    
    
    if (ticker >= 1){
        took++;
        if (took >= 100 && took < 400){circle0.radius +=0.5;}
        add(circle0);
    }
}

var circle0 = new Circle(1);
circle0.setPosition(200, 240);
circle0.setColor(Color.black);
add(circle0);

var ticker = -1;

function start(){
    mouseClickMethod(mouseClick);
    
}

function mouseClick(){
    ticker++;
     if (circle0.radius >= 151){
        stopTimer(animateBackground);
        
        circle0.radius = 1;anoTicker = 0;
        took = 0;ticker = 0;
    }
    
    setTimer(animateBackground,24);
}
